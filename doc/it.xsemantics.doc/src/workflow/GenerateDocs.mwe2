module GenerateSpec

import org.eclipse.emf.mwe.utils.*

var targetDir = "manual"
var modelPath = "xdoc"

Workflow {
	component = @workflow.XDocGenerator {
		// or define search scope explicitly
		modelPath = modelPath
		targetDir = targetDir
		document = "XSemantics Documentation"
		pdfLatex = "/usr/bin/pdflatex"
	}

}
